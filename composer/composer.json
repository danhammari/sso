{
    "name": "composer.sso.test",
    "description": "composer.sso.test",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "slim/slim": "^3.10",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "onelogin/php-saml": "^2.13",
        "league/oauth2-server": "~4.1",
        "lightsaml/lightsaml-idp": "^1.0",
        "google/apiclient": "^2.0",
        "facebook/graph-sdk": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FacebookSso\\": "/facebook/classes/",
            "GoogleSso\\": "/google/classes/",
            "Lti\\": "/lti/classes/",
            "Samlidp\\": "/samlidp/classes/",
            "Samlsp\\": "/samlsp/classes/"
        }
    }
}
